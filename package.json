    "eleventy",
    "eleventy-plugin"
  ],
  "author": {
    "name": "Ashur Cabrera",
    "url": "https://ashur.cab/rera"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashur/eleventy-plugin-attributes/issues"
  },
  "homepage": "https://github.com/ashur/eleventy-plugin-attributes#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@aaashur/eslint-config": "^1.0.0-beta.1",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "mocha": "^10.2.0",
    "onchange": "^7.1.0"
  },
  "eslintIgnore": [
    "!.*"
  ],
  "dependencies": {
    "@aaashur/eleventy-plugin-classnames": "^0.2.0",
    "@aaashur/eleventy-plugin-styles": "^0.2.0"
