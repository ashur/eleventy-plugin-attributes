{
  "name": "@aaashur/eleventy-plugin-attributes",
  "version": "1.0.0-beta.1",
  "description": "An Eleventy plugin for converting truthy object values into an HTML element attributes string",
  "main": ".eleventy.js",
  "scripts": {
    "lint": "eslint '.eleventy.js' 'src/**/*.js' 'test/**/*.js'",
    "test": "mocha ./test",
    "watch": "onchange 'src/**.js' 'test/**.js' -- npm run test"
